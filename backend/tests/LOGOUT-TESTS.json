{
  "client": "Thunder Client",
  "collectionName": "LOGOUT-TESTS",
  "dateExported": "2023-09-06T10:10:21.483Z",
  "version": "1.1",
  "folders": [],
  "requests": [
    {
      "name": "Logout - Successful",
      "method": "POST",
      "url": "http://localhost:3000/api/users/logout",
      "headers": [
        {
          "key": "Authorization",
          "value": "Bearer {{token}}"
        }
      ],
      "tests": [
        {
          "name": "Status Code is 200",
          "assert": "statusCode",
          "operator": "===",
          "value": 200
        },
        {
          "name": "Message is 'Logged out successfully'",
          "assert": "body.message",
          "operator": "===",
          "value": "Logged out successfully"
        }
      ]
    },
    {
      "name": "Logout - Missing Authorization Header",
      "method": "POST",
      "url": "http://localhost:3000/api/users/logout",
      "tests": [
        {
          "name": "Status Code is 401",
          "assert": "statusCode",
          "operator": "===",
          "value": 401
        },
        {
          "name": "Message is 'Unauthorized'",
          "assert": "body.message",
          "operator": "===",
          "value": "Unauthorized"
        }
      ]
    },
    {
      "name": "Logout - Invalid Token",
      "method": "POST",
      "url": "http://localhost:3000/api/users/logout",
      "headers": [
        {
          "key": "Authorization",
          "value": "Bearer invalid_token_here"
        }
      ],
      "tests": [
        {
          "name": "Status Code is 401",
          "assert": "statusCode",
          "operator": "===",
          "value": 401
        },
        {
          "name": "Message is 'Invalid token'",
          "assert": "body.message",
          "operator": "===",
          "value": "Invalid token"
        }
      ]
    }
  ]
}
